{
  "name": "rockett/weasyprint",
  "description": "A feature-rich Laravel wrapper for the WeasyPrint Document Factory.",
  "type": "library",
  "license": "ISC",
  "authors": [
    {
      "name": "Mike Rockett",
      "email": "os@rockett.pw"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^8.1",
    "illuminate/support": "^9.0|^10.0",
    "rockett/pipeline": "^2.0",
    "symfony/process": "^6.0"
  },
  "require-dev": {
    "johnbacon/stout": "^1.13",
    "nunomaduro/collision": "^7.0",
    "orchestra/testbench": "^8.0",
    "smalot/pdfparser": "^2.7"
  },
  "autoload": {
    "psr-4": {
      "WeasyPrint\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WeasyPrint\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "WeasyPrint\\Provider"
      ]
    }
  },
  "scripts": {
    "test": [
      "@dump",
      "@testbench"
    ],
    "dump": "composer dumpautoload",
    "testbench": "./vendor/bin/testbench package:test",
    "format": "./vendor/bin/stout"
  }
}
